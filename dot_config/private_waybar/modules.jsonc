{
  "mpris": {
    "format": "{player_icon} {dynamic}",
    "rotate": 0,
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "default": "‚ñ∂",
      "mpv": "üéµ"
    },
    "status-icons": {
      "paused": "ÔÅå"
    },
    "ignored-players": [
      "firefox",
      "zen-browser"
    ],
    "max-length": 100,
    "interval": 1
  },
  "privacy": {
    "icon-size": 10,
    "icon-spacing": 5,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 24
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 24
      }
    ]
  },
  "mpd": {
    "tooltip": true,
    "tooltip-format": "{artist} - {album} - {title} - Total Time : {totalTime:%M:%S}",
    "format": "Ôã≤ {elapsedTime:%M:%S}",
    "format-disconnected": "‚ö†  Disconnected",
    "format-stopped": "Ôã≤ Not Playing",
    "on-click": "mpc toggle",
    "state-icons": {
      "playing": "ÔÅã",
      "paused": "Ô££"
    }
  },
  "mpd#2": {
    "format": "ÔÅà",
    "format-disconnected": "ÔÅà",
    "format-paused": "ÔÅà",
    "format-stopped": "ÔÅà",
    // Commands to execute on events
    "on-click": "mpc -q pause && mpc -q prev && mpc -q start"
  },
  "mpd#3": {
    "interval": 1,
    "format": "{stateIcon}",
    "format-disconnected": "ÔÅã",
    "format-paused": "{stateIcon}",
    "format-stopped": "ÔÅã",
    "state-icons": {
      "paused": "ÔÅã",
      "playing": "ÔÅå"
    },
    // Commands to execute on events
    "on-click": "mpc toggle"
  },
  "mpd#4": {
    "format": "ÔÅë",
    "format-disconnected": "ÔÅë",
    "format-paused": "ÔÅë",
    "format-stopped": "ÔÅë",
    // Commands to execute on events
    "on-click": "mpc -q pause && mpc -q next && mpc -q start"
  },
  "custom/swap": {
    "on-click": "~/.config/waybar/scripts/swap.sh",
    "format": "Bg ÔàÑ ",
    "tooltip": true,
    "tooltip-format": "Swap between waybar Themes"
  },
  "custom/mpd": {
    "format": "ÔÄÅ {title} - {artist}",
    "interval": 5,
    "exec": "mpc current --format='\"title\":\"%title%\",\"artist\":\"%artist%\"'"
  },
  "custom/cava-internal": {
    "exec": "sh ~/.config/waybar/scripts/Cava.sh",
    "format": "{}",
    "tooltip": false,
    "on-click": "swaymsg reload & pkill waybar & waybar",
    "layer": "below",
    "output": "all"
  },
  "sway/mode": {
    "format": "<span style=\"italic\">{}</span>"
  },
  "sway/window": {
    "rewrite": {
      "(.*) - Brave": "$1",
      "(.*) - Chromium": "$1",
      "(.*) - Brave Search": "$1",
      "(.*) - Outlook": "$1",
      "(.*) Microsoft Teams": "$1"
    },
    "separate-outputs": true,
    "format": "<span color=\"#24273a\" style=\"italic\">{}</span>"
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "rotate": 0,
    "format-icons": {
      "activated": "ÔèÅ",
      "deactivated": "ÔÄ£"
    }
  },
  "tray": {
    "spacing": 5
  },
  "clock": {
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "timezone": "America/Argentina/Buenos_Aires",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#bb9af7'><b>{}</b></span>", // Purple
        "days": "<span color='#7aa2f7'><b>{}</b></span>", // Blue
        "weeks": "<span color='#9ece6a'><b>W{}</b></span>", // Green
        "weekdays": "<span color='#e0af68'><b>{}</b></span>", // Yellow
        "today": "<span background='#2ac3de' color='#1a1b26'><b><u>{}</u></b></span>" // Cyan background with dark text
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    },
    "format": "{:%a %d %b | %I:%M %p}", //12 hour format
    "format-alt": "ÔÑ≥  {:%Y-%m-%d ÔÄó %H:%M:%S}",
    "interval": 1
  },
  // System
  "custom/system": {
    "format": "Óë≥",
    "tooltip": false
  },
  "cpu": {
    "format": "Ôãõ {usage: >3}%",
    "on-click": "alacritty -e htop"
  },
  "memory": {
    "format": "Ôî∏ {: >3}%",
    "on-click": "alacritty -e htop"
  },
  "temperature": {
    "thermal-zone": 4,
    // "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 80,
    // "format-critical": "{temperatureC}¬∞C Ôãá",
    "format": "Ôãâ  {temperatureC}¬∞C"
  },
  // Harddisc space used
  "disk": {
    // "interval": 30,
    "format": "ÔÇ† {percentage_used}% ",
    "path": "/"
    //"on-click": "alacritty -e htop"
  },
  // Group Hardware
  "group/hardware": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 300,
      "children-class": "not-memory",
      "transition-left-to-right": false
    },
    "modules": [
      "custom/system",
      "disk",
      "cpu",
      "memory"
    ]
  },
  "backlight": {
    "format": "{icon} {percent: >3}%",
    "format-icons": [
      "Óéç",
      "Óèì",
      "Óèë",
      "Óèè",
      "Óèé",
      "Óèç",
      "Óèä",
      "Óèà",
      "Óéõ"
    ],
    "on-scroll-down": "brightnessctl -c backlight set 5%-",
    "on-scroll-up": "brightnessctl -c backlight set +5%",
    "on-click": ""
  },
  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{icon} {capacity: >3}%",
    "format-charging": "Ôóß  {capacity}%",
    "format-plugged": "Ôá¶  {capacity}%",
    "format-alt": "{icon}  {time}",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ]
  },
  "network": {
    // "interface": "wlp2s0", // (Optional) To force the use of this interface
    "format": "‚ö†  Disabled",
    "format-wifi": "Ôá´  {essid}",
    // "format-ethernet": "ÔÉÅ {ifname}: {ipaddr}/{cidr}",
    "format-ethernet": "ÔÉÅ  Wired",
    "format-disconnected": "‚ö†  Disconnected",
    "tooltip-format": "Ôûñ {ifname} via {gwaddri}",
    "tooltip-format-wifi": "Ôá´   {essid} ({signalStrength}%)",
    "tooltip-format-ethernet": "ÔÉÅ  {ifname} ({ipaddr}/{cidr})",
    "tooltip-format-disconnected": "‚ö†  Disconnected",
    // "on-click": "alacritty -e nmtui"
    "on-click": "nm-connection-editor"
  },
  "pulseaudio": {
    // "scroll-step": 5,
    "format": "{icon} {volume: >3}%",
    "format-bluetooth": "{icon} {volume: >3}%",
    "format-muted": "ÔÄ¶ muted",
    "format-icons": {
      "headphones": "ÔÄ•",
      "handsfree": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "on-click": "pavucontrol",
    // "on-scroll-up": "pamixer -ui 2 && pamixer --get-volume > $SWAYSOCK.wob",
    // "on-scroll-down": "pamixer -ud 2 && pamixer --get-volume > $SWAYSOCK.wob"
    "on-scroll-up": "pactl set-sink-volume @DEFAULT_SINK@ +5%",
    "on-scroll-down": "pactl set-sink-volume @DEFAULT_SINK@ -5%"
  },
  "custom/pacman": {
    "format": "ÔÜá {}",
    "interval": 7200, // every two hours
    "exec": "i=$(checkupdates); echo \"$i\" |wc -l; echo \"$i\" |column -t |tr '\n' '\r'", // # of updates and tooltip details
    "exec-if": "exit 0", // always run; consider advanced run conditions
    "on-click": "alacritty -e sudo pacman -Syu", // update system
    "signal": 8
  },
  "custom/power": {
    "format": "‚èª",
    "on-click": "wlogout -p layer-shell",
    "tooltip": false
  },
  "custom/launcher": {
    "format": "ÔÄï",
    "on-click": "rofi -show drun -replace",
    "tooltip": false
  },
